{
  "name": "Order",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "status": {
      "type": "string"
    },
    "amount": {
      "type": "number"
    },
    "price": {
      "type": "number"
    },
    "create_at": {
      "type": "date",
      "default": "$now"
    },
    "update_at": {
      "type": "date"
    },
    "create_by": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "product": {
      "type": "belongsTo",
      "model": "Product",
      "foreignKey": "productId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "*"
    },
   {
     "accessType": "*",
     "principalType": "ROLE",
     "principalId": "$unauthenticated",
     "permission": "DENY"
   },
   {
    "accessType": "READ",
    "principalType": "ROLE",
    "principalId": "$evreyone",
    "permission": "ALLOW"
  },
   {
     "accessType": "*",
     "principalType": "ROLE",
     "principalId": "$owner",
     "permission": "ALLOW"
   }
  ],
  "methods": {}
}
